		"svelte": "^5.38.6",
		"svelte-check": "^4.3.1",
		"tsx": "^4.20.5",
		"typescript": "^5.9.2",
		"vite": "^7.1.3",
		"vite-plugin-devtools-json": "^1.0.0",
